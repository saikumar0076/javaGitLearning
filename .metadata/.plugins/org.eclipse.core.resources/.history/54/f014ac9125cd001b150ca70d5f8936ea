package operatorsAndExpressions;

public class Arithmetic2 {

	public static void main(String[] args) {
		float x = 14.3f,y=3.2f;
		float r = x%y;
		float z = x*y;
		int a=25;
		float b=2.5f;
		float c=a/b;
		char p=40;
		int q=30;
		int d= p-q;
		
		System.out.println(r);
		System.out.println(z);
		System.out.println(c);
		System.out.println(d);

	}

}
