package javaProgrammingLearning;

public class AddSub {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int a = 4, b=5;
		float c = a/b;
		int myNum = 14;
		c=myNum;
		System.out.println("sum is equal to "+(a+b));
		System.out.println("sub is equal to "+(a-b));
		System.out.println("my num is "+c);
		System.out.println("mult is "+ (a*b));
		
	}

}
